{
	"name": "image-comments",
	"displayName": "Image Comments",
	"icon": "icon.png",
	"version": "0.5.0",
	"description": "This Visual Studio Code extension lets you easily add visual comments such as sketches or diagrams directly into your source code, improving code documentation and understanding. Support js python cpp csharp ts tsx php java.",
	"publisher": "mgiesen",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/Wugaulk/Image-Comments.git"
	},
	"engines": {
		"vscode": "^1.60.0"
	},
	"categories": [
		"Programming Languages",
		"Formatters",
		"Other"
	],
	"activationEvents": [
		"onLanguage:javascript",
		"onLanguage:python",
		"onLanguage:cpp",
		"onLanguage:csharp",
		"onLanguage:typescript",
		"onLanguage:typescriptreact",
		"onLanguage:php",
		"onLanguage:java"
	],
	"main": "./extension.js",
	"contributes": {
		"hoverProvider": [
			{
				"for": [
					"javascript",
					"python",
					"cpp",
					"csharp",
					"typescript",
					"typescriptreact",
					"php",
					"java"
				],
				"type": "markdown"
			}
		]
	},
	"devDependencies": {
		"vscode": "^1.1.37"
	}
}
